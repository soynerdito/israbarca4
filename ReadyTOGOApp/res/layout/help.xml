	<Button android:paddingBottom="10dp" android:layout_marginBottom="10dp" android:layout_marginRight="10dp" android:layout_width="100dp" android:layout_height="40dp" android:background="@drawable/view_orderbutton" android:id="@+id/Button03"  ></Button>
	<Button android:paddingBottom="10dp" android:layout_marginBottom="10dp" android:layout_marginRight="10dp" android:layout_width="100dp" android:layout_height="40dp" android:background="@drawable/call_waiterbutton" android:id="@+id/Button05" ></Button>
	<Button android:paddingBottom="10dp" android:layout_marginBottom="10dp" android:layout_marginRight="10dp" android:layout_width="100dp" android:layout_height="40dp" android:background="@drawable/helpbutton" android:id="@+id/Button04"></Button>
	

</LinearLayout>
<LinearLayout android:layout_x="80dp" android:layout_width="wrap_content" android:layout_height="fill_parent" android:id="@+id/LinearLayout03" android:layout_y="50dp">
<TextView android:id="@+id/TextView02" android:textColor="#FFFFFF" android:gravity="fill_horizontal" android:layout_height="fill_parent" android:paddingRight="100dp" android:paddingLeft="20dp" android:text="" 
android:layout_width="wrap_content" android:layout_y="125dip"></TextView>

</LinearLayout>

